<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" prefHeight="380.0" prefWidth="380.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxdemo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label fx:id="sendMessage" />
      <TextField fx:id="firstNameTextField" alignment="CENTER" promptText="Förnamn" style="-fx-border-color: blue; -fx-border-width: 1; -fx-border-radius: 4;" />
      <TextField fx:id="lastNameTextField" alignment="CENTER" layoutX="30.0" layoutY="30.0" promptText="Efternamn" style="-fx-border-color: blue; -fx-border-width: 1; -fx-border-radius: 4;" />
      <TextField fx:id="emailTextField" alignment="CENTER" layoutX="30.0" layoutY="77.0" promptText="E-mail" style="-fx-border-color: blue; -fx-border-width: 1; -fx-border-radius: 4;" />
      <DatePicker fx:id="datePicker" promptText="Datum för  tidsbokning" />
      <Button fx:id="sendButton" focusTraversable="false" mnemonicParsing="false" onAction="#onSendButtonClick" text="Skicka Förfrågan" textFill="#3300ff" />
   </children>
</VBox>
